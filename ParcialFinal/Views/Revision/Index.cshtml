@model List<ParcialFinal.Models.ViewModels.ListRevisionViewModel>
@{
    ViewBag.Title = "Revisiones";
}

<h2>@ViewBag.Title</h2>

<div class="row" style="text-align:right;padding:5px;">
    <a href="~/Revision/Nuevo" class="btn btn-primary">Nuevo</a>
</div>

<div class="row">
    <div class="col-md-12">
        <table class="table">
            <tr>
                <th scope="col">#</th>
                <th scope="col">ID Cliente</th>
                <th scope="col">ID Empleado</th>
                <th scope="col">ID Producto</th>
                <th scope="col">Observaciones</th>
                <th scope="col">Fecha Revision</th>
            </tr>
            @foreach (var oL in Model)
            {
                <tr>
                    <td scope="row">@oL.Id</td>
                    <td scope="row">@oL.Cliente_Id</td>
                    <td scope="row">@oL.Empleado_Id</td>
                    <td scope="row">@oL.Producto_Id</td>
                    <td scope="row">@oL.Observaciones</td>
                    <td scope="row">@oL.Fecha_Revision</td>
                    <td>
                        <a class="btn btn-default" href="~/Revision/Editar/@oL.Id">Editar</a>
                        <a class="btn btn-danger" href="~/Revision/Eliminar/@oL.Id">Eliminar</a>
                    </td>
                </tr>
            }
        </table>
    </div>
</div>

<div class="row" style="text-align:right;padding:5px;">
    <br />
    <a class="btn btn-warning" href="~/">Regresar</a>
    <br />
    <br />
</div>

